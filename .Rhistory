credit$MARRIAGE <- factor(credit$MARRIAGE,
levels = 1:3,
labels = c("Đã cưới", "Độc thân", "Khác"))
credit$SEX <- factor(credit$SEX,
levels = 0:1,
labels = c("Nữ", "Nam"))
credit$DEFAULT <-factor(credit$DEFAULT)
independent_var <- names(credit)[!names(credit) %in% c("ID", "DEFAULT")]
uni_formular <- lapply(independent_var,
function(x){as.formula(paste0("DEFAULT ~", x))})
glm(DEFAULT ~ PAY_AMT1, data = credit, family = "binomial") %>%
glance()
uni_logistic <- lapply(uni_formular, brglm,
data = credit,
family = "binomial")
est_result <- uni_logistic %>% lapply(tidy)
names(est_result) <- independent_var
fit_result <- uni_logistic %>% lapply(augment)
test_result <- uni_logistic %>% sapply(glance) %>%
t() %>% as.data.frame() %>%
cbind(variable = independent_var)
do.call("rbind", est_result) %>% filter(term != "(Intercept)",
p.value <= 0.05)
